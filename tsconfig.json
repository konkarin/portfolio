{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "lib": ["ESNext", "ESNext.AsyncIterable", "DOM"],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "types": ["@types/node", "@types/jest", "@types/uuid", "@types/lodash.debounce"]
  },
  "exclude": ["node_modules", "functions"]
}
